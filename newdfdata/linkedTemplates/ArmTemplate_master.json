{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "newdfdata"
		},
		"11092022_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of '11092022'"
		},
		"blob_to_blob_linkedservice_copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_to_blob_linkedservice_copy'"
		},
		"blobltobloblinkedinservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobltobloblinkedinservice'"
		},
		"emplyeesalary_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'emplyeesalary'"
		},
		"emplyeesalary1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'emplyeesalary1'"
		},
		"newcloudprem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'newcloudprem'"
		},
		"newlinkedlistsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'newlinkedlistsource'"
		},
		"newlinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'newlinkedservice'"
		},
		"sinklinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sinklinkedservice'"
		},
		"sinklinkedservices_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sinklinkedservices'"
		},
		"sourcedatasetlinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sourcedatasetlinkedservice'"
		},
		"sqltosqllinkedservices_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqltosqllinkedservices'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "newdfdata_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"11092022_connectionString": {
						"value": "[parameters('11092022_connectionString')]"
					},
					"blob_to_blob_linkedservice_copy_connectionString": {
						"value": "[parameters('blob_to_blob_linkedservice_copy_connectionString')]"
					},
					"blobltobloblinkedinservice_connectionString": {
						"value": "[parameters('blobltobloblinkedinservice_connectionString')]"
					},
					"emplyeesalary_connectionString": {
						"value": "[parameters('emplyeesalary_connectionString')]"
					},
					"emplyeesalary1_connectionString": {
						"value": "[parameters('emplyeesalary1_connectionString')]"
					},
					"newcloudprem_connectionString": {
						"value": "[parameters('newcloudprem_connectionString')]"
					},
					"newlinkedlistsource_connectionString": {
						"value": "[parameters('newlinkedlistsource_connectionString')]"
					},
					"newlinkedservice_connectionString": {
						"value": "[parameters('newlinkedservice_connectionString')]"
					},
					"sinklinkedservice_connectionString": {
						"value": "[parameters('sinklinkedservice_connectionString')]"
					},
					"sinklinkedservices_connectionString": {
						"value": "[parameters('sinklinkedservices_connectionString')]"
					},
					"sourcedatasetlinkedservice_connectionString": {
						"value": "[parameters('sourcedatasetlinkedservice_connectionString')]"
					},
					"sqltosqllinkedservices_connectionString": {
						"value": "[parameters('sqltosqllinkedservices_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "newdfdata_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'newdfdata_ArmTemplate_0')]"
			]
		}
	]
}